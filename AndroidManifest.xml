<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="ie.simo.movies.activities" android:versionCode="3" android:versionName="0.13">
	<uses-sdk android:minSdkVersion="8" />

	<!-- uses-permission android:name="android.permission.INTERNET"/> -->
	<application android:icon="@drawable/ic_launcher"
		android:label="@string/app_name">
		<activity android:label="@string/app_name" android:name=".StartActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:label="Choose Film" android:name=".MakeFilmActivity">
		</activity>
		<activity android:label="Choose Director" android:name=".GetDirector">
		</activity>
		<activity android:label="Box Office" android:name=".BoxOffice"
			android:screenOrientation="landscape">
		</activity>
		<activity android:label="Create your film" android:name=".OwnFilmActivity">
		</activity>
		<activity android:label="Result" android:name=".Result">
		</activity>
	</application>
</manifest>